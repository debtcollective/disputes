{
  "name": "@debtcollective/tools",
  "version": "0.7.0",
  "description": "Disputes app tools",
  "author": "Orlando Del Aguila <orlando@hashlabs.com>",
  "homepage": "https://github.com/debtcollective/disputes#readme",
  "license": "BSD-3-Clause",
  "main": "src/index.js",
  "directories": {
    "src": "src",
    "test": "__tests__"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/debtcollective/disputes.git"
  },
  "scripts": {
    "pre-commit": "lint-staged",
    "test": "jest",
    "test:ci": "yarn test:coverage",
    "test:coverage": "jest --coverage --verbose --color",
    "test:watch": "jest --watch"
  },
  "bugs": {
    "url": "https://github.com/debtcollective/disputes/issues"
  },
  "dependencies": {
    "@date-io/moment": "1.3.8",
    "@material-ui/core": "3.9.3",
    "@material-ui/icons": "3.0.2",
    "lodash": "4.17.15",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-jsonschema-form": "1.7.0",
    "react-number-format": "4.0.8"
  },
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@babel/plugin-transform-runtime": "7.5.5",
    "babel-eslint": "10.0.2",
    "babel-jest": "24.8.0",
    "babel-plugin-styled-components": "1.10.6",
    "core-js": "3",
    "cypress": "3.4.0",
    "eslint": "6.0.1",
    "eslint-plugin-flowtype": "3.11.1",
    "eslint-plugin-import": "2.18.0",
    "eslint-plugin-jest": "22.7.2",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.14.2",
    "flow-bin": "0.102.0",
    "husky": "3.0.0",
    "jest": "24.8.0",
    "jest-dom": "3.5.0",
    "lint-staged": "9.1.0",
    "react-testing-library": "7.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "flow focus-check",
      "eslint --fix",
      "git add"
    ],
    "*.{json,md}": [
      "prettier --fix",
      "git add"
    ]
  }
}
