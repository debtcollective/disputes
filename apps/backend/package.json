{
  "name": "@debtcollective/disputes-backend",
  "version": "0.8.0",
  "description": "Dispute Tools back-end",
  "main": "index.js",
  "license": "3-clause BSD",
  "private": true,
  "scripts": {
    "build": "babel src -d build",
    "db:migrate": "yarn knex migrate:latest",
    "db:migrate:down": "yarn knex migrate:down",
    "db:test:prepare": "NODE_ENV=test yarn db:migrate",
    "now-build": "yarn build && yarn setup",
    "pre-commit": "lint-staged",
    "setup": "cp knexfile.sample.js knexfile.js",
    "start": "nodemon --exec babel-node src/index.js",
    "test": "jest --watch",
    "test:ci": "yarn test:coverage",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "apollo-server-express": "2.6.7",
    "express": "4.17.1",
    "graphql": "14.4.2",
    "graphql-type-json": "~0.3.0",
    "knex": "0.19.0",
    "lodash": "~4.17.11",
    "objection": "1.6.9",
    "pg": "7.11.0"
  },
  "devDependencies": {
    "@babel/cli": "7.5.5",
    "@babel/core": "7.5.5",
    "@babel/node": "7.5.5",
    "@babel/preset-env": "7.5.5",
    "babel-eslint": "10.0.2",
    "babel-jest": "24.8.0",
    "babel-plugin-import-graphql": "2.7.0",
    "babel-plugin-module-resolver": "~3.2.0",
    "eslint": "6.0.1",
    "eslint-plugin-flowtype": "3.11.1",
    "eslint-plugin-graphql": "3.0.3",
    "eslint-plugin-import": "2.18.0",
    "eslint-plugin-jest": "22.7.2",
    "eslint-plugin-node": "9.1.0",
    "eslint-plugin-promise": "4.2.1",
    "flow-bin": "0.102.0",
    "jest": "24.8.0",
    "lint-staged": "9.1.0",
    "nodemon": "1.19.1",
    "prettier-eslint-cli": "5.0.0"
  },
  "lint-staged": {
    "*.js": [
      "prettier-eslint --write",
      "flow focus-check",
      "git add"
    ],
    "*.{json,md}": [
      "prettier-eslint --write",
      "git add"
    ]
  }
}
