{
  "name": "disputes",
  "version": 2,
  "builds": [
    { "src": "apps/backend/src/index.js", "use": "@now/node-server" },
    { "src": "apps/frontend/package.json", "use": "@now/next" }
  ],
  "routes": [
    { "src": "/graphql(.*)", "dest": "apps/backend/src/index.js" },
    { "src": "/(.*)", "dest": "apps/frontend/$1" }
  ]
}
